{
    "portraitScale": 1.59,
    "portraitTranslation": [-0.2, 1.87, 0],
    "profileScale": 0.55,
    "profileTranslation": [0, 0.95, 0],
  "rootBone": "canjitsu",
  "firstPersonCameraOffset": {
    "seat_1": [0, 0.45, 0]
  },
  "thirdPersonCameraOffset": {
    "seat_1": [0, 0.35, 0],
    "seat_1_reverse": [0, 0.35, 0]
  },
  "thirdPersonCameraOffsetNoViewBobbing": {
    "seat_1": [0, 0.35, 0],
    "seat_1_reverse": [0, 0.35, 0]
  },
    "animations": {
      "faint": "q.bedrock_primary('canjitsu', 'faint', q.curve('one'))",
      "cry": "q.bedrock_stateful('canjitsu', 'cry')",
      "recoil": "q.bedrock_stateful('canjitsu', 'recoil')"
    },
  "poses": {
    "standing": {
      "poseName": "standing",
      "poseTypes": ["STAND", "FLOAT", "NONE", "PORTRAIT", "PROFILE"],
      "isBattle": false,
      "condition": "!q.in_air",
      "animations": [
        "q.look('head_ai')",
        "q.bedrock('canjitsu', 'ground_idle')"
      ],
      "quirks": [
        "q.bedrock_quirk('canjitsu', 'blink')"
      ],
      "transformTicks": 10
    },
    "battle-standing": {
      "poseTypes": ["STAND"],
      "isBattle": true,
      "condition": "!q.in_air",
      "animations": [
        "q.look('head_ai')",
        "q.bedrock('canjitsu', 'battle_idle')"
      ],
      "namedAnimations": {
        "cry": "q.bedrock_primary('canjitsu', 'battle_cry', q.curve('symmetrical_wide'))"
      },
      "quirks": ["q.bedrock_quirk('canjitsu', 'blink')"]
    },
    "in-air": {
      "poseTypes": ["STAND", "WALK"],
      "isBattle": false,
      "condition": "q.in_air && q.is_ridden",
      "animations": [
        "q.look('head_ai')",
        "q.bedrock('canjitsu', 'ride_jump')",
        "q.bedrock('canjitsu', 'ride_ground_run')"
      ],
      "transformTicks": 2,
      "transformToTicks": 2
    },
    "sleep": {
      "poseTypes": ["SLEEP"],
      "animations": [
        "q.bedrock('canjitsu', 'sleep')"
      ],
      "namedAnimations": {
        "cry": "q.bedrock_stateful('dummy', 'cry')"
      }
    },
    "walk": {
      "poseName": "walk",
      "poseTypes": ["WALK"],
      "condition": "!q.in_air && !q.is_sprinting",
      "animations": [
        "q.look('head_ai')",
        "q.bedrock('canjitsu', 'ground_walk')",
        {
          "condition": "q.is_ridden && q.has_entity",
          "animation": "q.bedrock('canjitsu', 'ride_ground_walk')"
        }
      ],
      "quirks": [
        "q.bedrock_quirk('canjitsu', 'blink')"
      ],
      "transformTicks": 5
    },
    "run": {
      "poseName": "run",
      "poseTypes": ["STAND","WALK"],
      "condition": "!q.in_air && q.is_sprinting",
      "animations": [
        "q.look('head_ai')",
        "q.bedrock('canjitsu', 'ground_run')",
        {
          "condition": "q.is_ridden && q.has_entity",
          "animation": "q.bedrock('canjitsu', 'ride_ground_run')"
        }
      ],
      "quirks": [
        "q.bedrock_quirk('canjitsu', 'blink')"
      ],
      "transformTicks": 5
    }
  }
}
