        "q.look('head_right_ai', 1, 1, 45, -45, 20, -20)",
        "q.look('head_ai', 1, 1, 45, -45, 20, -20)",
        "q.bedrock('branchief', 'ground_run')",
        {
          "condition": "q.is_ridden && q.has_entity",
          "animation": "q.bedrock('branchief', 'ride_ground_run')"
        }
      ],
      "quirks": [
          "q.bedrock_quirk('branchief', 'blink1')",
          "q.bedrock_quirk('branchief', 'blink2')",
          "q.bedrock_quirk('branchief', 'blink3')"
      ],
      "transformTicks": 5
    }
