        "q.look('head_left_ai', 1, 1, 45, -45, 20, -20)",
        "q.look('head_right_ai', 1, 1, 45, -45, 20, -20)",
        "q.look('head_ai', 1, 1, 45, -45, 20, -20)",
        "q.bedrock('branchief', 'ground_walk')",
        {
          "condition": "q.is_ridden && q.has_entity",
          "animation": "q.bedrock('branchief', 'ride_ground_walk')"
        }
      ],
      "quirks": [
          "q.bedrock_quirk('branchief', 'blink1')",
