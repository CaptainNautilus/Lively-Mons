        "q.look('head_right_ai', 1, 1, 45, -45, 20, -20)",
        "q.look('head_ai', 1, 1, 45, -45, 20, -20)",
        "q.bedrock('branchief', 'ride_jump')",
        "q.bedrock('branchief', 'ride_ground_run')"
      ],
      "transformTicks": 2,
      "transformToTicks": 2
    },
    "sleep": {
      "poseTypes": ["SLEEP"],
      "animations": [
        "q.bedrock('branchief', 'sleep')"
      ],
      "namedAnimations": {
