{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "cobblemon:cyclopod_cyclone_shiny",
			"basic_render_parameters": {
				"material": "particles_alpha",
				"texture": "textures/particles/cyclone"
			}
		},
		"components": {
			"minecraft:emitter_rate_steady": {
				"spawn_rate": 500,
				"max_particles": 10000
			},
			"minecraft:emitter_lifetime_looping": {
				"active_time": 99999
			},
			"minecraft:emitter_shape_point": {
				"offset": [0, 0, 0],
				"direction": [
					"math.cos(v.particle_random_1 * 360)",
					"1.0",
					"math.sin(v.particle_random_1 * 360)"
				]
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": 0.5
			},
			"minecraft:particle_initial_spin": {
				"rotation_rate": 0
			},
			"minecraft:particle_initial_speed": "math.random(1.5, 2.5)",
			"minecraft:particle_motion_dynamic": {
				"linear_acceleration": [
					"math.cos(v.particle_age * 600 + v.particle_random_1 * 360) * 7.0 + math.cos(v.particle_age * 600 + v.particle_random_1 * 360) * 2.0",
					"1.25",
					"math.sin(v.particle_age * 600 + v.particle_random_1 * 360) * 7.0 + math.sin(v.particle_age * 600 + v.particle_random_1 * 360) * 2.0"
				],
				"linear_drag_coefficient": 0.5
			},
			"minecraft:particle_appearance_billboard": {
				"size": ["0.1 + v.particle_age * 0.2", "0.1 + v.particle_age * 0.2"],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 8,
					"texture_height": 64,
					"flipbook": {
						"base_UV": [0, 0],
						"size_UV": [8, 8],
						"step_UV": [0, 8],
						"frames_per_second": 15,
						"max_frame": 8,
						"stretch_to_lifetime": true
					}
				}
			},
			"minecraft:particle_appearance_lighting": {},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime",
					"gradient": {
						"0.0": "#FF0f081d",
						"0.6": "#FF28117a",
						"1.0": "#FFf4f4f5"
					}
				}
			}
		}
	}
}